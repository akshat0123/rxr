<!DOCTYPE html>
<html>
  <head>
    <title>Cart</title>
		<% include header.ejs %>
  </head>
  <body>
		<% include navbar.ejs %>

		<div class='container-fluid'>

			<div class='row'>
				<div class='col-md-2'><h3>Item</h3></div>
				<div class='col-md-6'></div>
				<div class='col-md-2'><h3>Price</h3></div>
				<div class='col-md-2'><h3>Quantity</h3></div>
			</div>
			<% for (var i = 0; i < items.length; i++) { %>
				<div class='row'>
					<div class='panel panel-default'>
						<div class='panel-heading'></div>
						<div class='panel-body'>
							<div class='col-md-2'>
								<img class='img-responsive center-block' src='<%= items[i].image %>'/>
							</div>
							<div class='col-md-6'>
								<h3><%= items[i].title %></h3>
							</div>
							<div class='col-md-2'>
								<h3><%= items[i].price %></h3>
							</div>
							<div class='col-md-2'>
								<h3>1</h3>
							</div>
						</div>
					</div>
				</div>
			<% } %>

		</div>

  </body>
	<script src='/javascripts/jquery/jquery.min.js'></script>
	<script src='/javascripts/jquery/jquery.min.map'></script>
	<script src='/javascripts/bootstrap/bootstrap.min.js'></script>
</html>
